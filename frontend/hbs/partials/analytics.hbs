<amp-analytics type="googleanalytics" id="analytics">
  <script type="application/json">
    {
      "vars": {
        "account": "{{ config.shared.gaTrackingId }}"
      },
      "triggers": {
        "default pageview": {
          "on": "visible",
          "request": "pageview",
          "vars": {
            "title": "{{ title }}"
          }
        }
      }
    }
  </script>
</amp-analytics>
